class  Operators
{
	public static void main(String[] args) 
	{
		byte a=10;
		byte b=5;

		System.out.println("a and b add is :"+(a+b));
		System.out.println("a and b sub is :"+(a-b));
		System.out.println("a and b multi is :"+(a*b));
		System.out.println("a and b div is :"+(a/b));
		System.out.println("a and b remainder is :"+(a%b));
	}
}
